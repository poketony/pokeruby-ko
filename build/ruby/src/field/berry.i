    if (berry > ITEM_ENIGMA_BERRY - ITEM_CHERI_BERRY)
        return ((ITEM_CHERI_BERRY - ITEM_CHERI_BERRY) + 1);
    else
        return ((item - ITEM_CHERI_BERRY) + 1);
}

static u16 BerryTypeToItemId(u16 berry)
